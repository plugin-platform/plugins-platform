{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"allowJs": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"jsx": "preserve",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"baseUrl": ".",
		"strict": true,
		"paths": {},
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"noImplicitAny": false
	}
}
